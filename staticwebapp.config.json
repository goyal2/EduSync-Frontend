{
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*",
            "/css/*",
            "/js/*",
            "/api/*"
        ]
    },
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "globalHeaders": {
        "cache-control": "no-cache",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}